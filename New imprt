CREATE PLUGGABLE DATABASE pdb_us7ascii
ADMIN USER pdbadmin IDENTIFIED BY PdbAdmin123
ROLES = (DBA)
DEFAULT TABLESPACE users
DATAFILE 'C:\Oracle21c\oradata\ORCL\pdb_us7ascii_users01.dbf' SIZE 500M AUTOEXTEND ON
FILE_NAME_CONVERT = ('ORCLPDB', 'pdb_us7ascii')
PATH_PREFIX = 'C:\Oracle21c\oradata\pdb_us7ascii\'
CHARACTER SET US7ASCII
NATIONAL CHARACTER SET AL16UTF16;



ALTER PLUGGABLE DATABASE pdb_us7ascii OPEN;


ALTER PLUGGABLE DATABASE pdb_us7ascii SAVE STATE;


